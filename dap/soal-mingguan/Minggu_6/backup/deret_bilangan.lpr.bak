program deret_bilangan;
uses crt;
var
  bil1,bil2,min,max,bil:integer;
begin
  clrscr;
  write('masukkan a: ');readln(bil1);
  write('masukkan b: ');readln(bil2);

  if (a>=b) then
  begin
    min:=b;
    max:=a;
  end
  else
  begin
    min:=a;
    max:=b;
  end;
  for bil:=min to max do
  begin
    write(bil,' ');
  end;
  writeln;
  readln;
end.
